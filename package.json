{
  "name": "zod-build-json-schemas",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Zapotinschii Augustin",
  "license": "MIT",
  "description": "Wrapper around zod-to-json-schema, that makes it easier to use it with Fastify & OpenAPI.",
  "scripts": {
